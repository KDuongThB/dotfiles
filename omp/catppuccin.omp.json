{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "sapphire": "#74C7EC",
    "green": "#A6E3A1",
    "red": "#F38BA8",
    "maroon": "#EBA0AC",
    "mauve": "#CBA6F7",
    "peach": "#FAB387",
    "yellow": "#f9e2af",
    "rosewater": "#F5E0DC",
    "text": "#CDD6F4"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:mauve",
          "template": "╭── "
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:text",
          "properties": {
            "time_format": "02 Jan 03:04:05 A.M."
          },
          "template": "{{ .CurrentDate | date .Format }} "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:os",
          "template": "<></> {{ .FormattedMs }}",
          "properties": {
            "always_enabled": true,
            "threshold": 1,
            "style": "roundrock"
          }
        }
      ],
      "type": "prompt"
    },

    {
      "type": "prompt",
      "alignment": "left",
      "newline": false,
      "overflow": "hide",
      "filler": "<p:mauve>─</>",
      "segments": []
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:mauve",
          "template": "├"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:mauve",
          "template": "── "
        },
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}}{{ if .WSL }} WSL{{ end }} ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "p:peach",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:green",
          "fetch_package_manager": true,
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ else }}󰎙 {{ end }}{{ if .Error }}{{ .Error }}{{ end }}{{ .Full }}"
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:red",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:sapphire",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:mauve",
          "template": "╰── "
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "󱂵",
            "style": "agnoster_short",
            "mixed_threshold": 5
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "style": "plain",
          "foreground": "p:red",
          "template": "\uf105",
          "type": "text"
        },
        {
          "style": "plain",
          "foreground": "p:sapphire",
          "template": "\uf105",
          "type": "text"
        },
        {
          "style": "plain",
          "foreground": "p:green",
          "template": "\uf105",
          "type": "text"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2
}
